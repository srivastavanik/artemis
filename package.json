{
  "name": "artemis",
  "version": "1.0.0",
  "description": "AI-powered B2B sales intelligence platform with multi-agent architecture",
  "main": "backend/src/index.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "nodemon backend/src/index.js",
    "dev:frontend": "cd frontend && vite",
    "build": "npm run build:frontend",
    "build:frontend": "cd frontend && vite build",
    "start": "node backend/src/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "db:migrate": "node database/migrate.js",
    "db:seed": "node database/seed.js",
    "db:reset": "npm run db:migrate && npm run db:seed"
  },
  "keywords": [
    "ai",
    "sales",
    "intelligence",
    "b2b",
    "multi-agent",
    "brightdata",
    "llamaindex",
    "arcade",
    "mastra",
    "supabase"
  ],
  "author": "Nikhil Srivastava",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.39.3",
    "axios": "^1.6.5",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.3",
    "redis": "^4.6.12",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.4",
    "vite": "^5.0.12"
  }
}
